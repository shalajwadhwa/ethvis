{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "outDir": "dist",
    "target": "ES2017",
    "isolatedModules": false,
    "noEmit": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["server.ts", "lib/**/*.js", "app/lib/server/**/*.ts"]
}
